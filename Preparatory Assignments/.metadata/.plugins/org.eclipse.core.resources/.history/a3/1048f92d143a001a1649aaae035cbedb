#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(void)
{
	char s[20]="HINJEWADI";
	char d[20]="sunbeam ";
	printf("s : %s\n",s);
	printf("d : %s\n",d);


	printf("\nAfter Copy : ");
	printf("s : %s\n",s);
	printf("d : %s\n",d);
	return EXIT_SUCCESS;
}
