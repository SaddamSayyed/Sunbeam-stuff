#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(void)
{
	char s[20]="sunbeam";
	char d[20]="hinjewadi";
	printf("s : %s\n",s);
	printf("d : %s\n",d);
	strcpy(s,d);
	printf("\nAfter strcpy :\n");
	printf("s : %s\n",s);
	printf("d : %s\n",d);
	return EXIT_SUCCESS;
}
